// SEN-08 Humidity Display
// Simulated humidity reading (analog humidity sensor)

#define HUM_PIN 32

void setup() {
  Serial.begin(9600);
}

void loop() {
  int raw = analogRead(HUM_PIN);
  int humidity = map(raw, 0, 4095, 0, 100);

  Serial.print("Humidity: ");
  Serial.print(humidity);
  Serial.println(" %");

  delay(1000);
}
