// SEN-12 IR Object Detection
// Detects nearby object using IR sensor

#define IR_PIN 26
#define LED 2

void setup() {
  pinMode(IR_PIN, INPUT);
  pinMode(LED, OUTPUT);
}

void loop() {
  if (digitalRead(IR_PIN) == LOW) {
    digitalWrite(LED, HIGH); // Object detected
  } else {
    digitalWrite(LED, LOW);
  }

  delay(200);
}
