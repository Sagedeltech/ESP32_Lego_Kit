// SEN-16 Reed Switch Detection
// Detects magnetic open/close

#define REED_PIN 27

void setup() {
  Serial.begin(9600);
  pinMode(REED_PIN, INPUT_PULLUP);
}

void loop() {
  if (digitalRead(REED_PIN) == LOW) {
    Serial.println("Magnet Detected (Closed)");
  } else {
    Serial.println("Open");
  }
  delay(500);
}
